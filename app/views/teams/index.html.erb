<% provide(:title,"Teams") %>
<h1>Team lists</h1>
<% @teams.each do |team| %>
<div class="span4">
	<h4><%= team.name %></h4>
	<p><%= team.description %> </p>
	<strong>Members</strong>
	<div>
		<ul>
		<% team.users do |user| %>
		<li><%= user.name %></li>
		<% end %>
		</ul>
	</div>
</div>

<% end %>


